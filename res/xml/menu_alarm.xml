<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android" >

	<PreferenceCategory
		android:title="@string/sms_alarm">
		<CheckBoxPreference
		    android:key="checkbox_sms"
			android:title="@string/title_cb_sms"
			android:summary="@string/summary_cb_sms" />
		
		<EditTextPreference
			android:key="edittext_sms_phonenumber"
