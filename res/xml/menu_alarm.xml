			android:summary="@string/summary_et_sms_phonenumber"
			android:dialogTitle="@string/dialog_title_et_sms_phonenumber" />
		
		<EditTextPreference
			android:key="edittext_sms_message"
			android:title="@string/title_et_sms_message"
			android:summary="@string/summary_et_sms_message"
			android:dialogTitle="@string/dialog_title_et_sms_message" />
